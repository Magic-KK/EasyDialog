<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.custom.easydialog">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OneBtnActivity"
            android:label="默认一个按钮" />
        <activity
            android:name=".TwoBtnActivity"
            android:label="默认二个按钮" />
        <activity
            android:name=".OneBtnCustomizeActivity"
            android:label="自定义一个按钮" />
        <activity
            android:name=".TwoBtnCustomzeActivity"
            android:label="自定义二个按钮" />
        <activity android:name=".MoreBtnCustomizeActivity"></activity>
    </application>

</manifest>